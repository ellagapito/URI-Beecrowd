#include <iostream>
using namespace std;
int main (){
    
    string nome1, nome2, nome3;
    
    cin >> nome1 >> nome2 >> nome3;
    
    if(nome1 == "vertebrado" and nome2 == "ave" and nome3 == "carnivoro"){
        cout << "aguia" << endl;
    }
        else{
            if(nome1 == "vertebrado" and nome2 == "ave" and nome3 == "onivoro"){
            cout << "pomba" << endl;
            }
    else{
        
    if(nome1 == "vertebrado" and nome2 == "mamifero" and nome3 == "onivoro"){
        cout << "homem" << endl;
    }
        else{
            if(nome1 == "vertebrado" and nome2 == "mamifero" and nome3 == "herbivoro"){
            cout << "vaca" << endl;
            }
    else{
        
    if(nome1 == "invertebrado" and nome2 == "inseto" and nome3 == "hematofago"){
        cout << "pulga" << endl;
    }
        else{
            if(nome1 == "invertebrado" and nome2 == "inseto" and nome3 == "herbivoro"){
            cout << "lagarta" << endl;
            }
        else{
            
     if(nome1 == "invertebrado" and nome2 == "anelideo" and nome3 == "hematofago"){
        cout << "sanguessuga" << endl;
    }
        else{
            if(nome1 == "invertebrado" and nome2 == "anelideo" and nome3 == "onivoro"){
            cout << "minhoca" << endl;
            }
        }
        }
        }
    }
        }
    }
    }
}
